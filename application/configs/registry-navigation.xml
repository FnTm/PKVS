				<operators>
					<label>Operatori</label>
					<module>admin</module>
					<controller>registry</controller>
					<action>operators</action>
				</operators>
				<operators-add>
					<label>Pievienot Operatoru</label>
					<module>admin</module>
					<controller>registry</controller>
