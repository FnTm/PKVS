				<service>
					<label>Servisi</label>
					<module>admin</module>
					<controller>registry</controller>
					<action>services</action>
				</service>
				<add-service>
					<label>Pievienot servisu</label>
					<module>admin</module>
					<controller>registry</controller>
					<action>service-add</action>
				</add-service>
				<process>
					<label>Procesi</label>
					<module>admin</module>
					<controller>registry</controller>
					<action>process</action>
				</process>
				<add-process>
					<label>Pievienot procesu</label>
					<module>admin</module>
					<controller>registry</controller>
					<action>process-add</action>
